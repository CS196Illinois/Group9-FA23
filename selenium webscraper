from selenium import webdriver

website = 'https://www.tradingview.com/screener/?aff_id=4191'
path = 'C:/Users/sherr/Downloads/chromedriver_win32/chromedriver.exe'
driver = webdriver.Chrome(path)
driver.get(website)
table = driver.find_element(By.CLASS_NAME, "tv-data-table__tbody")
companies = driver.find_elements_by_tag('tr')

for company in companies:
    print(company.text)
